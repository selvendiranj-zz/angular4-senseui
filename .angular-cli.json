{
    "project": {
        "name": "angular2-senseui"
    },
    "apps": [{
        "root": "src",
        "outDir": "dist",
        "assets": [
            "assets",
            "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "test": "../../scripts/test.ts",
        "tsconfig": "tsconfig.json",
        "prefix": "",
        "mobile": false,
        "styles": [
            "../node_modules/bootstrap/dist/css/bootstrap.min.css",
            "assets/css/style.css",
            "../node_modules/font-awesome/css/font-awesome.min.css",
            "../bower_components/switchery/dist/switchery.min.css",
            "../bower_components/bootstrap-select/dist/css/bootstrap-select.min.css",
            "../bower_components/bootstrapvalidator/dist/css/bootstrapValidator.min.css",
            "../plugins/jvectormap/jquery-jvectormap.css",
            "../bower_components/jquery-steps/demo/css/jquery.steps.css",
            "../bower_components/pace/themes/green/pace-theme-center-radar.css"
        ],
        "scripts": [
            "../bower_components/pace/pace.min.js",
            "../node_modules/jquery/dist/jquery.min.js",
            "../node_modules/bootstrap/dist/js/bootstrap.min.js",
            "../bower_components/fastclick/lib/fastclick.js",
            "../bower_components/switchery/dist/switchery.min.js",
            "../bower_components/parsleyjs/dist/parsley.min.js",
            "../bower_components/jquery-steps/build/jquery.steps.min.js",
            "../bower_components/bootstrap-select/dist/js/bootstrap-select.min.js",
            "../plugins/bootstrap-wizard/jquery.bootstrap.wizard.min.js",
            "../plugins/masked-input/bootstrap-inputmask.min.js",
            "../bower_components/bootstrapvalidator/dist/js/bootstrapValidator.min.js",
            "../plugins/flot-charts/jquery.flot.min.js",
            "../plugins/flot-charts/jquery.flot.resize.min.js",
            "../plugins/flot-charts/jquery.flot.categories.js",
            "../plugins/jvectormap/jquery-jvectormap.min.js",
            "../plugins/jvectormap/jquery-jvectormap-us-aea-en.js",
            "../plugins/easy-pie-chart/jquery.easypiechart.min.js",
            "../plugins/screenfull/screenfull.js"
        ],
        "environmentSource": "environments/environment.ts",
        "environments": {
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
        }
    }],
    "addons": [],
    "packages": [],
    "e2e": {
        "protractor": {
            "config": "protractor.conf.js"
        }
    },
    "test": {
        "karma": {
            "config": "karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "css",
        "prefixInterfaces": false,
        "inline": {
            "style": false,
            "template": false
        },
        "spec": {
            "class": false,
            "component": true,
            "directive": true,
            "module": false,
            "pipe": true,
            "service": true
        }
    }
}