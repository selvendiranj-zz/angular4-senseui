{
    "project": {
        "name": "ng2-bootstrap"
    },
    "apps": [{
        "root": "src",
        "outDir": "dist",
        "assets": [
            "assets",
            "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "test": "../../scripts/test.ts",
        "tsconfig": "tsconfig.json",
        "prefix": "",
        "mobile": false,
        "styles": [
            "../node_modules/bootstrap/dist/css/bootstrap.min.css",
            "assets/css/glyphicons.css",
            "assets/css/style.css",
            "assets/css/prettify-angulario.css"
        ],
        "scripts": [
            "../node_modules/google-code-prettify/bin/prettify.min.js"
        ],
        "environmentSource": "environments/environment.ts",
        "environments": {
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
        }
    }],
    "addons": [],
    "packages": [],
    "e2e": {
        "protractor": {
            "config": "protractor.conf.js"
        }
    },
    "test": {
        "karma": {
            "config": "karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "css",
        "prefixInterfaces": false,
        "inline": {
            "style": false,
            "template": false
        },
        "spec": {
            "class": false,
            "component": true,
            "directive": true,
            "module": false,
            "pipe": true,
            "service": true
        }
    }
}