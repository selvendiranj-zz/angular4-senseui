{
    "name": "angular2-deepdive",
    "version": "1.4.2",
    "description": "Native Angular Bootstrap Components",
    "private": true,
    "scripts": {
        "lite-server": "lite-server -c bs-config.js",
        "typedoc": "typedoc --exclude '**/*.spec.ts' ./src/",
        "demo.docs": "node scripts/docs/get-doc.js",
        "demo.serve": "run-s build link demo.build lite-server",
        "demo.gh-pages": "run-s build demo.build demo.deploy",
        "demo.build": "ng build -prod --aot && npm run generate-bs4",
        "demo.deploy": "gh-pages -d dist",
        "demo-beta-deploy": "gh-pages -d dist -r git@github.com:valorkin/ng2-bootstrap.git -b gh-pages",
        "link": "ngm link -p src --here",
        "lint": "exit 0",
        "disable-lint": "tslint \"**/*.ts\" -c tslint.json --fix --type-check -t prose -e \"node_modules/**\" -e \"dist/**\" -e \"temp/**\" -e \"scripts/docs/**\"",
        "flow.changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
        "flow.github-release": "conventional-github-releaser -p angular",
        "build": "ngm build -p src --clean",
        "build.watch": "ngm build -p src --watch --skip-bundles",
        "start": "ng serve --aot --host localhost",
        "generate-bs4": "node scripts/generate-bs4.js",
        "pretest": "run-s lint build link",
        "test": "ng test -sr",
        "test-cross": "SAUCE=true ng test -sr",
        "test-coverage": "ng test -sr -cc",
        "postversion": "run-s changelog git-add git-commit-ammend",
        "git-add": "git add -A",
        "git-commit-ammend": "git commit --amend --no-edit -n",
        "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular -r 2",
        "pree2e": "webdriver-manager update",
        "e2e": "protractor",
        "e2e-cross": "SAUCE=true npm run e2e",

        "ls:build": "tsc -p src/",
        "ls:build-watch": "tsc -p src/ -w",
        "ls:build-e2e": "tsc -p e2e/",
        "ls:serve": "lite-server -c=bs-config.js",
        "ls:serve-e2e": "lite-server -c=bs-config.e2e.json",
        "ls:prestart": "npm run ls:build",
        "ls:start": "concurrently \"npm run ls:build-watch\" \"npm run ls:serve\"",
        "ls:pree2e": "npm run ls:build-e2e",
        "ls:e2e": "concurrently \"npm run ls:serve-e2e\" \"npm run ls:protractor\" --kill-others --success first",
        "ls:preprotractor": "webdriver-manager update",
        "ls:protractor": "protractor protractor.config.js",
        "ls:pretest": "npm run ls:build",
        "ls:test": "concurrently \"npm run ls:build-watch\" \"karma start karma.conf.js\"",
        "ls:pretest-once": "npm run build",
        "ls:test-once": "karma start karma.conf.js --single-run",
        "ls:lint": "tslint ./src/**/*.ts -t verbose"
    },
    "main": "bundles/ng2-bootstrap.umd.js",
    "module": "index.js",
    "typings": "index.d.ts",
    "keywords": [
        "ng",
        "ng-bootstap",
        "angular",
        "angular2",
        "bootstrap",
        "twitter-bootstrap"
    ],
    "author": "Dmitriy Shekhovtsov <valorkin@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/valor-software/ng2-bootstrap.git"
    },
    "bugs": {
        "url": "https://github.com/valor-software/ng2-bootstrap/issues"
    },
    "homepage": "https://github.com/valor-software/ng2-bootstrap#readme",
    "dependencies": {
        "moment": "2.18.1",
        "ng2-bootstrap": "1.6.0"
    },
    "peerDependencies": {
        "@angular/animations": "^4.0.0",
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/language-service": "^4.0.0",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/router": "^4.0.0",
        "bootstrap": "3.3.7",
        "core-js": "^2.4.1",
        "zone.js": "0.8.5",
        "rxjs": "5.2.0"
    },
    "devDependencies": {
        "@angular/animations": "^4.0.0",
        "@angular/cli": "1.0.0",
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/compiler-cli": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/language-service": "^4.0.0",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/router": "^4.0.0",
        "@angular/tsc-wrapped": "4.0.0",
        "@types/jasmine": "2.5.47",
        "@types/marked": "0.0.28",
        "@types/node": "7.0.12",
        "@types/webpack": "^2.2.13",
        "classlist-polyfill": "1.0.3",
        "codecov": "2.1.0",
        "codelyzer": "3.0.0-beta.4",
        "conventional-changelog-cli": "1.3.1",
        "conventional-github-releaser": "1.1.3",
        "cpy": "5.0.0",
        "cpy-cli": "1.0.1",
        "del-cli": "0.2.1",
        "gh-pages": "0.12.0",
        "gitignore-to-glob": "0.3.0",
        "google-code-prettify": "1.0.5",
        "html-loader": "0.4.5",
        "jasmine": "2.5.3",
        "jasmine-core": "2.5.2",
        "jasmine-data-provider": "2.2.0",
        "jasmine-spec-reporter": "3.2.0",
        "karma": "1.5.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.1.0",
        "karma-remap-istanbul": "0.6.0",
        "karma-sauce-launcher": "1.1.0",
        "lite-server": "2.3.0",
        "lodash": "4.17.4",
        "markdown-loader": "2.0.0",
        "marked": "0.3.6",
        "ng2-page-scroll": "4.0.0-beta.6",
        "ngm-cli": "0.4.4",
        "npm-run-all": "4.0.2",
        "protractor": "5.1.1",
        "reflect-metadata": "0.1.10",
        "require-dir": "0.3.1",
        "systemjs": "0.20.10",
        "ts-helpers": "^1.1.2",
        "tslint": "4.5.1",
        "tslint-config-valorsoft": "1.2.0",
        "typedoc": "0.5.9",
        "typescript": "2.2.2",
        "wallaby-webpack": "0.0.38",
        "webdriver-manager": "12.0.4",
        "webpack-bundle-analyzer": "2.3.1"
    }
}