// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": { "when": "$(basename).ts" },
        "**/*.js.map": { "when": "$(basename).map" },
        "npm-debug.log": true
    },
    "vsicons.presets.angular": true
}